@model string
@{

    Layout = "~/Views/Shared/CodefoxLayout.cshtml";
   
}

@Model
<!-- Content Column -->
<div class="container-fluid">
    <div class="col-lg-12 mb-4">

        <!-- Project Card Example -->
        <div class="page-title-box">
            <h3 class="m-0 font-weight-bold text-primary">Token Infomation</h3><br />

        </div>
        <div class="card card-primary card-outline">
            <div class="card-body">
                <form class="d-none d-sm-inline-block form-inline mr-auto ml-md-3 my-2 my-md-0 mw-100 navbar-search">
                    <div class="input-group">
                        <div class="input-group-append">
                            <button type="button" class="btn btn-danger" onclick="generateCodes()">Generate Codes</button>
                        </div>
                    </div>
                </form>
                <div class="table-responsive">
                    <table class="table table-bordered" id="" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th style="white-space: nowrap;">Token ID</th>
                                <th style="white-space: nowrap;">Token Code</th>
                                <th style="white-space: nowrap;">Access Token</th>
                                <th style="white-space: nowrap;">Refresh Token</th>
                                <th style="white-space: nowrap;">Country User ID</th>
                                <th style="white-space: nowrap;">Country Seller ID</th>
                                <th style="white-space: nowrap;">Country Short Code</th>
                                <th style="white-space: nowrap;">Refresh Token Status</th>
                                <th style="white-space: nowrap;">Active Token Status</th>


                            </tr>
                        </thead>
                        <tbody>
                            @if (ViewBag.TokenInfo == null)
                            {
                                <tr>
                                    <td colspan="9" style="text-align:center">
                                        No Token Available!
                                    </td>
                                </tr>
                            }
                            else
                            {

                                @foreach (var tokenInfo in (IEnumerable<TokenClass>)ViewData["TokenInfo"])
                                {

                                    <tr>
                                        <td>@tokenInfo.ids</td>
                                        <td>@tokenInfo.codeToGetToken</td>
                                        <td>@tokenInfo.access_token</td>
                                        <td>@tokenInfo.refresh_token</td>
                                        <td>@tokenInfo.country_user_id</td>
                                        <td>@tokenInfo.country_seller_id</td>
                                        <td>@tokenInfo.country_short_code</td>
                                        <td>@tokenInfo.access_expires_in</td>
                                        <td>@tokenInfo.refresh_expires_in</td>

                                    </tr>


                                }

                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
</div>


@section Scripts {
    <script>
        $("#ribbonTag").text("Lazada Token")
    </script>

    <script src="@Url.Content("~/Scripts/Token.js")" type="text/javascript"></script>
    @* <script src="~/Scripts/User.js" type="text/javascript"></script>*@


}