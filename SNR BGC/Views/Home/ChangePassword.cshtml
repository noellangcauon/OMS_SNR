@*<body class="bg-gradient-primary">*@
<body>
    <div class="container">
        <div class="row justify-content-center account-pages mt-5 mb-5">
            <div class="col-md-8 col-lg-6 col-xl-4">
                <div class="card shadow-none">

                    <div class="card-body p-3">

                        <div class="text-center w-75 m-auto">
                            <div class="auth-logo">
                                <a href="index.html" class="logo logo-dark text-center">
                                    <span class="logo-lg">
                                        <img src="~/img/snr.png" alt="" height="100">
                                    </span>
                                </a>

                                <a href="index.html" class="logo logo-light text-center">
                                    <span class="logo-lg">
                                        <img src="assets/images/logo-light.png" alt="" height="22">
                                    </span>
                                </a>
                            </div>
                            <p class="text-muted mb-4 mt-3">Change password for first time user.</p>
                        </div>


                        <form class="user">
                            <div class="input-group border" style="border-radius: 10rem;">
                                <input type="password" name="password" placeholder="Current password..." id="txtPassword" class="form-control form-control-user border-0" />
                                <div class="input-group-append hover-effect">
                                    <button class="btn " style="border-radius: 0rem 10rem 10rem 0rem;" id="btnShowPassword" type="button">
                                        <span class="bi bi-eye"></span>
                                    </button>
                                </div>
                            </div>

                            <br />
                            <div class="input-group border" style="border-radius: 10rem;">
                                <input type="password" name="password" placeholder="New password..." id="txtPasswordNew" class="form-control form-control-user border-0" />
                                <div class="input-group-append hover-effect">
                                    <button class="btn " style="border-radius: 0rem 10rem 10rem 0rem;" id="btnShowPasswordNew" type="button">
                                        <span class="bi bi-eye"></span>
                                    </button>
                                </div>
                            </div>
                            <div id="lblUppercase" class="d-none">
                                <span class="text-danger ml-md-2">•Password must have Uppercase</span>
                            </div>
                            <div id="lblLowercase" class="d-none">
                                <span class="text-danger ml-md-2">•Password must have Lowercase</span>
                            </div>
                            <div id="lblSymbols" class="d-none">
                                <span class="text-danger ml-md-2">•Password must have Symbols</span>
                            </div>
                            <div id="lblNumbers" class="d-none">
                                <span class="text-danger ml-md-2">•Password must have Numbers</span>
                            </div>
                            <div id="lblLength" class="d-none">
                                <span class="text-danger ml-md-2">•Password must have atleast 8 characters</span>
                            </div>
                            <br />

                            <div class="input-group border" style="border-radius: 10rem;">
                                <input type="password" name="password" placeholder="Confirm password..." id="txtPasswordConfirm" class="form-control form-control-user border-0" />

                            </div>

                            <br />
                        </form>

                        <button class="btn btn-primary btn-user btn-block" id="btnChangePassword" style="border-radius:16px;">Submit</button>

                        <div class="alert alert-danger d-none" style="margin-top:2%;" id='errorDiv'>
                            <strong>Failed to proceed!</strong> <span id="errorLabel"></span>
                        </div>


                    </div> <!-- end card-body -->
                </div>
                <!-- end card -->
                <!-- end row -->

            </div> <!-- end col -->
        </div>
        <!-- end row -->
